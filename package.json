{
  "name": "expresso-typeorm",
  "version": "2.6.0",
  "description": "Express built in TypeORM",
  "main": "./src/server.ts",
  "scripts": {
    "key:generate": "npx ts-node ./src/scripts/generate.ts",
    "dev": "yarn && npm-run-all build start:dev",
    "start": "nodemon --exec 'ts-node' ./src/server.ts",
    "start:dev": "nodemon --exec node ./dist/server.js",
    "clean": "rm -rf ./dist",
    "test:lint": "npx eslint '*/**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "test:lint-windows": "npx eslint */**/*.{js,jsx,ts,tsx} --quiet --fix",
    "test:types": "tsc --noEmit",
    "test:types-watch": "npm run test:types -- --watch",
    "prebuild": "npm-run-all clean test:lint test:types",
    "build": "npx swc src -d dist",
    "build:ts": "npm run prebuild && npx tsc -p .",
    "deploy": "npm run build && pm2 reload ecosystem.config.js",
    "serve:staging": "NODE_ENV=staging node ./dist/server.js",
    "serve:production": "NODE_ENV=production node ./dist/server.js",
    "serve:staging-docker": "NODE_ENV=staging pm2-runtime ./dist/server.js",
    "serve:production-docker": "NODE_ENV=production pm2-runtime ./dist/server.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "db:sync": "npm run typeorm -- schema:sync -d ./src/database/data-source.ts",
    "db:drop": "npm run typeorm -- schema:drop -d ./src/database/data-source.ts",
    "db:create": "npx ts-node ./src/scripts/createDB.ts",
    "db:destroy": "npx ts-node ./src/scripts/dropDB.ts",
    "db:reset": "yarn && npm-run-all db:drop db:sync",
    "prerelease": "npm-run-all prebuild",
    "release": "standard-version",
    "release:pre": "npm run release -- --prerelease",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "masb0ymas",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">= 14.x"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.216.0",
    "@aws-sdk/s3-request-presigner": "^3.216.0",
    "axios": "^1.1.3",
    "bcrypt": "^5.1.0",
    "chalk": "4.1.2",
    "compression": "^1.7.4",
    "convert-excel-to-json": "^1.7.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.29.2",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-useragent": "^1.0.15",
    "firebase": "^9.14.0",
    "firebase-admin": "^11.2.1",
    "googleapis": "^109.0.1",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.0",
    "hpp": "^0.2.3",
    "i18next": "^22.0.6",
    "i18next-fs-backend": "^2.0.0",
    "i18next-http-middleware": "^3.2.1",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^8.5.1",
    "libphonenumber-js": "^1.10.14",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.8.0",
    "nodemailer-mailgun-transport": "^2.1.4",
    "npm-run-all": "^4.1.5",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "rate-limiter-flexible": "^2.4.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "slugify": "^1.6.5",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.10",
    "typeorm-extension": "^2.2.12",
    "uuid": "^9.0.0",
    "winston": "^3.8.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.20",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/convert-excel-to-json": "^1.7.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-useragent": "^1.0.2",
    "@types/hpp": "^0.2.2",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.190",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.9",
    "@types/node-cron": "^3.0.2",
    "@types/nodemailer": "^6.4.5",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "@types/redis": "^2.8.32",
    "@types/request-ip": "^0.0.37",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.2",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "_moduleAliases": {
    "@expresso/*": "dist/@expresso/*",
    "@config/*": "dist/config/*",
    "@controllers/*": "dist/controllers/*",
    "@database/*": "dist/database/*",
    "@jobs/*": "dist/jobs/*",
    "@middlewares/*": "dist/middlewares/*",
    "@routes/*": "dist/routes/*",
    "@scripts/*": "dist/scripts/*",
    "@views/*": "dist/views/*"
  }
}
